{
  "presets": [
    "@emotion/babel-preset-css-prop",
    "@babel/preset-react",
    ["@babel/preset-env", {
      "loose": true,
      "shippedProposals": true
    }]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    ["module-resolver", {
      "alias": {
        "@mocks": "./__mocks__",
        "@root": "./src",
        "@routes": "./src/routes",
        "@api": "./src/api",
        "@assets": "./src/assets",
        "@components": "./src/components",
        "@containers": "./src/containers",
        "@constants": "./src/constants",
        "@config": "./src/config",
        "@data": "./src/data",
        "@hooks": "./src/hooks",
        "@lang": "./src/lang",
        "@styles": "./src/styles",
        "@types": "./src/types",
        "@utils": "./src/utils"
      }
    }],
    ["react-intl-auto", {
      "removePrefix": "src",
      "filebase": false
    }],
    "@emotion",
    "babel-plugin-styled-components"
  ],
  "env": {
    "test": {
      "plugins": ["require-context-hook"]
    }
  }
}
