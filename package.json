    "@material-ui/core": "4.12.3",
    "@material-ui/lab": "4.0.0-alpha.60",
    "@mdi/js": "6.5.95",
    "@mdi/react": "1.5.0",
    "axios": "0.25.0",
    "caniuse-lite": "1.0.30001312",
