{
  "name": "if.japanese",
  "version": "0.0.1",
  "private": true,
  "keywords": [
    "React",
    "Babel"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/furdzik/if.japanese.git"
  },
  "bugs": {
    "url": "https://github.com/furdzik/if.japanese/issues"
  },
  "homepage": "https://github.com/furdzik/if.japanese#readme",
  "description": "Japanese APP",
  "scripts": {
    "start": "npm run lang && npm run dev",
    "build": "npx webpack --env.production --progress --colors",
    "lint": "npm run eslint && npm run stylelint",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu npx jest --config jest.config.json --coverage --bail --updateSnapshot",
    "lang": "extract-messages -l=en -d=en -o lang/generated --flat \"src/**/**/**/*.messages.js\"",
    "validate": "npm run lint && npm run find-duplicates",
    "dev": "npx webpack-dev-server --history-api-fallback --env.development --open",
    "lint:fix": "npm run eslint:fix && npm run stylelint:fix",
    "eslint": "eslint src/ --ext .jsx,.js",
    "eslint:ci": "npm run eslint -- --quiet -f json -o reports/eslint-report.json",
    "eslint:fix": "npm run eslint -- --fix",
    "find-duplicates": "jscpd ./src/",
    "serve": "npx serve ./dist",
    "storybook:copyfiles": "npx copy ./.storybook/public/* ./storybook-static/",
    "storybook": "npm run lang && start-storybook -p 6006 -s ./.storybook/public",
    "stylelint:fix": "npm run stylelint -- --fix",
    "stylelint:ci": "npm run stylelint -- --formatter json --output-file reports/stylelint-report.json",
    "stylelint": "npx stylelint \"**/*.styles.js\""
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lang",
      "pre-push": "npm run validate"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@formatjs/intl-pluralrules": "1.5.2",
    "@formatjs/intl-relativetimeformat": "4.5.9",
    "@mdi/js": "4.9.95",
    "@mdi/react": "1.3.0",
    "axios": "0.21.1",
    "core-js": "3.6.4",
    "formik": "2.1.4",
    "furigana": "0.19.3",
    "lodash": "4.17.21",
